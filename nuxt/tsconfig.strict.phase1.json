{
  "extends": "./.nuxt/tsconfig.app.json",
  "files": [
    "./.nuxt/types/imports.d.ts",
    "./.nuxt/types/app.config.d.ts",
    "./.nuxt/types/runtime-config.d.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": false,
    "checkJs": false,
    "useUnknownInCatchVariables": true,
    "types": ["node"]
  },
  "include": [
    "app/features/article-admin/**/*.ts",
    "app/features/article-admin/**/*.vue",
    "app/features/tutorials/utils/**/*.ts",
    "app/features/gallery-public/utils/**/*.ts"
  ]
}
